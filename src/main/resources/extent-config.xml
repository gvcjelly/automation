<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
<configuration>
    <!-- setup theme -->
    <!-- standard, dark -->
    <theme>dark</theme>

    <!-- document encoding -->
    <!-- defaults to UTF-8 -->
    <encoding>UTF-8</encoding>

    <!-- protocol for script and stylesheets -->
    <!-- defaults to https -->
    <protocol>https</protocol>

    <!-- title of the document -->
    <documentTitle>MS2 Automation</documentTitle>

    <!-- setup name - displayed at top-nav -->
    <reportName> <![CDATA[ <img src='../src/main/resources/logo.png' />  ]]>
    </reportName>

    <!-- location of charts in the test view -->
    <!-- top, bottom -->
    <testViewChartLocation>top</testViewChartLocation>

    <!-- custom javascript -->
    <scripts>
        <![CDATA[
                $(document).ready(function() {

                });
            ]]>
    </scripts>

    <!-- custom styles -->
    <styles>
        <![CDATA[
               .report-name { padding-left:85px; }
               .report-name > img { float:left; width:auto; position:fixed; }
            ]]>
    </styles>
</configuration>
</extentreports>